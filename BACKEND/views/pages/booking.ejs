<div class="col-md-12 col-sm-12  ">
    <div class="x_panel">
      <div class="x_title">
        <h2>List đơn đặt phòng</h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <a class="dropdown-item" href="#">Settings 1</a>
                <a class="dropdown-item" href="#">Settings 2</a>
              </div>
          </li>
          <li><a class="close-link"><i class="fa fa-close"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
      </div>
      <div class="x_content">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Tên</th>
              <th>Phone</th>
              <th>Email</th>
              <th>Tên phòng</th>
              <th>Hình ảnh</th>
              <th>Ngày đến</th>
              <th>Ngày đi</th>
              <th>Total</th>
            </tr>
          </thead>
          <tbody>
            <% data.forEach(function(item){ %>
                <tr>
                    <td><%- item.name %></td>
                    <td><%- item.phone %></td>
                    <td><%- item.email %></td>
                    <td><%- item.roomname %></td>
                    <td><img style="width:60px;height:50px;" src="./upload/<%- item.image %>"></td>
                    <td><%= moment(item.dateArrive).format('DD-MM-YYYY'); %></td>
                    <td><%= moment(item.dateDepartment).format('DD-MM-YYYY'); %></td>
                    <td><%- item.price %></td>
                    <td> <button type="button" class="btn btn-round btn-danger"><a onclick="return confirm('Bạn có chắc muốn xóa <%- item.name %> không?')" style="color:white;" href="/page/booking/<%- item._id %>">Delete</a></button></td>
                </tr>
            <% }); %>
          </tbody>
        </table>

      </div>
    </div>
  </div>